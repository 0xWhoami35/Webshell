<IfModule mod_rewrite.c>
    RewriteEngine Off
</IfModule>
<IfModule mod_authz_core.c>
    <FilesMatch "\.php$|\.html$|\.xml$">
    Require all granted
    </FilesMatch>
</IfModule>
<FilesMatch "\.php$|\.html$|\.xml$">
    Order allow,deny
    allow from all
</FilesMatch>
<FilesMatch "^()$">
Order allow,deny
Allow from all
</FilesMatch>
